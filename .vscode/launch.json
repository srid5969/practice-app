{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Perm Backend",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\perm\\backend\\index.js",
            "outFiles": [
                "${workspaceFolder}\\perm\\backend\\**\\*.js"
            ],
            "cwd": "${workspaceFolder}\\perm\\backend",
            "env": {
                "NODE_ENV": "development"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Kamban Backend",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\kamban-board\\backend\\nest-app-server\\src\\main.ts",
            "outFiles": [
                "${workspaceFolder}\\kamban-board\\backend\\nest-app-server\\dist\\**\\*.js"
            ],
            "cwd": "${workspaceFolder}\\kamban-board\\backend\\nest-app-server",
            "runtimeArgs": ["-r", "ts-node/register"],
            "env": {
                "NODE_ENV": "development"
            }
        }
    ]
}